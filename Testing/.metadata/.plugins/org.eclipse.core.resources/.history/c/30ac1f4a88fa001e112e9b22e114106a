#ifndef INC_PID_H_
#define INC_PID_H_

typedef struct gains {
	float Kp;
	float Ki;
	float Kd;
} gains;

typedef struct signals {
	float current_error;
	float old_error;
	float errot_integral;
	float error_derivative;
	int control;
};




#endif /* INC_PID_H_ */
